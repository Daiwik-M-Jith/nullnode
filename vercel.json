{
  "version": 2,
  "name": "nullsector-site",
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "learn/**", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" },
    { "src": "terminal.js", "use": "@vercel/static" },
    { "src": "logo.svg", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
